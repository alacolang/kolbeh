{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "node lib/index.js",
    "start:messaging-producer": "node lib/messaging/producer.js",
    "start:messaging-worker": "node lib/messaging/workder.js",
    "build": "tsc -p .",
    "generate": "graphql-codegen",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "dev": "yarn dev:app dev:messaging",
    "dev:messaging": "yarn dev:messaging-producer dev:messaging-worker",
    "dev:app": "nodemon --watch 'src/**/*.ts' --exec \"ts-node\" src/index.ts",
    "dev:messaging-producer": "nodemon  --exec \"ts-node\" src/messaging/cron.ts",
    "dev:messaging-worker": "nodemon  --exec \"ts-node\" src/messaging/worker.ts",
    "fmt": "prettier --write 'src/**/*.*'",
    "optimize-images": "node scripts/optimize-images.js"
  },
  "dependencies": {
    "@babel/register": "^7.9.0",
    "apollo-server": "^2.11.0",
    "apollo-server-express": "^2.11.0",
    "apollo-server-plugin-response-cache": "^0.4.1",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "bull": "^3.18.1",
    "bull-board": "^0.10.0",
    "chalk": "^4.1.0",
    "cors": "^2.8.5",
    "date-fns": "^2.16.1",
    "date-fns-tz": "^1.0.12",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-basic-auth": "^1.2.0",
    "firebase-admin": "^9.3.0",
    "front-matter": "^3.1.0",
    "fs-extra": "^9.0.0",
    "graphql": "^14.6.0",
    "joi": "^17.3.0",
    "marked": "^0.8.2",
    "md5": "^2.2.1",
    "p-limit": "^2.3.0",
    "ramda": "^0.27.0",
    "redis": "^3.0.2",
    "sharp": "^0.25.2",
    "typescript": "^4.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-typescript": "^7.12.7",
    "@graphql-codegen/cli": "^1.19.1",
    "@graphql-codegen/typescript": "^1.17.11",
    "@types/bull-board": "^0.6.0",
    "@types/cron": "^1.7.2",
    "@types/express": "^4.17.9",
    "@types/express-serve-static-core": "^4.17.13",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^26.0.19",
    "@types/marked": "^1.1.0",
    "@types/md5": "^2.2.1",
    "@types/node": "^14.14.7",
    "@types/ramda": "^0.27.32",
    "@types/redis": "^2.8.28",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "eslint": "^7.13.0",
    "jest": "^26.6.3",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.2",
    "prettier": "^2.0.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  }
}
